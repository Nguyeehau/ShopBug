@model List<ShopBug.Model.Models.Menu>

<div class="container">
    <div class="header">
        <div class="col-md-9 header-left">
            <div class="top-nav">
                <ul class="memenu skyblue">
                    @foreach (var item in Model)
                    {
                        <li class="grid"><a href="@item.URL" target="@item.Target">@item.Name</a></li>
                    }

                </ul>
            </div>
            <div class="clearfix"> </div>
        </div>
        <div class="col-md-3 header-right">
            <div class="search-bar">
                <input type="text" placeholder="Search.." value="Search" onfocus="this.value = '';" {this.value='Search' ;}">
                <input type="submit" value="">
            </div>
        </div>
        <div class="clearfix"> </div>
    </div>
</div>
